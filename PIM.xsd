            <xs:element name="SpecialCharacterTagGroup" type="SpecialCharacterTagGroupType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="CharacterTagGroup" type="CharacterTagGroupType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="FootnoteTagGroup" type="FootnoteTagGroupType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="HyperlinkTagGroup" type="HyperlinkTagGroupType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>



    <xs:complexType name="TagListType">
        <xs:annotation>
            <xs:documentation>
                List of all tags in the system.
            </xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Tag" type="TagType">
                <xs:annotation>
                    <xs:documentation>
                        DEPRECATED. For backward compatibility. Contains all the step tags only.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StyleTag" type="StyleTagType">
                <xs:annotation>
                    <xs:documentation>
                        A style tag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SpecialCharacterTag" type="SpecialCharacterTagType">
                <xs:annotation>
                    <xs:documentation>
                        A special character tag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CharacterTag" type="CharacterTagType">
                <xs:annotation>
                    <xs:documentation>
                        A character tag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FootnoteTag" type="FootnoteTagType">
                <xs:annotation>
                    <xs:documentation>
                        A footnote tag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="HyperlinkTag" type="HyperlinkTagType">
                <xs:annotation>
                    <xs:documentation>
                        A hyperlink tag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="TagType">
        <xs:annotation>
            <xs:documentation>
                DEPRECATED. For backward compatibility. Contains all the step tags only.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Start" type="xs:string" use="required"/>
        <xs:attribute name="Stop" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="StyleTagType">
        <xs:annotation>
            <xs:documentation>
                Represents a style tag.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        How tag content should be styled in STEP GUI.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TagFormat" type="TagFormatType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="TagID" use="required">
            <xs:annotation>
                <xs:documentation>
                    ID of tag - in STEP GUI presented as Name of tag. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Tag" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Tag name. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent tag group. If not supplied and tag not already exists it will be created on top level.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="SpecialCharacterTagType">
        <xs:annotation>
            <xs:documentation>
                Represents a special character tag.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Formatting" type="FormattingType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:hexBinary" use="required">
            <xs:annotation>
                <xs:documentation>
                    ID of tag - in STEP GUI presented as Code of tag. Four character hexadecimal representation of character.
                    The character (not the hexadecimal code) must be unique across all tags.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent tag group. If not supplied and tag not already exists it will be created on top level.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="CharacterTagType">
        <xs:annotation>
            <xs:documentation>
                Represents a character tag.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        How tag content should be styled in STEP GUI.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Formatting" type="FormattingType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="TagID" use="required">
            <xs:annotation>
                <xs:documentation>
                    ID of tag - in STEP GUI presented as Name of tag. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Tag" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Tag name. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="GuiRendering" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    String displayed in STEP GUI for tag.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent tag group. If not supplied and tag not already exists it will be created on top level.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="FootnoteTagType">
        <xs:annotation>
            <xs:documentation>
                Represents a footnote tag.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TagFormat" type="TagFormatType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="TagID" use="required">
            <xs:annotation>
                <xs:documentation>
                    ID of tag - in STEP GUI presented as Name of tag. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Tag" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Tag name. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent tag group. If not supplied and tag not already exists it will be created on top level.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="HyperlinkTagType">
        <xs:annotation>
            <xs:documentation>
                Represents a hyperlink tag.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        How tag content should be styled in STEP GUI.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TagFormat" type="TagFormatType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="TagID" use="required">
            <xs:annotation>
                <xs:documentation>
                    ID of tag - in STEP GUI presented as Name of tag. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Tag" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Tag name. Must be unique across all tag types.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent tag group. If not supplied and tag not already exists it will be created on top level.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="TagID">
        <xs:restriction base="xs:string">
            <xs:maxLength value="2000"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="TagFormatType">
        <xs:annotation>
            <xs:documentation>
                Tag output conversions. Describes how tag is outputted to a given format.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FormattingStart" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        How tag should be converted to given output format.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FormattingStop" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        How end tag should be converted to given output format. Not relevant for character and special character tags.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Format" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Output format.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="FormattingType" mixed="true">
        <xs:annotation>
            <xs:documentation>
                Tag output conversions. Describes how tag is outputted to a given format.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Format" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Output format.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="TargetUserTypeLinkType">
        <xs:attribute name="UserTypeID" type="StepObjectTypeID" use="required"/>
    </xs:complexType>


    <xs:complexType name="TemplateLayoutType">
        <xs:attribute name="TemplateID" type="StepID" use="required"/>
    </xs:complexType>


    <xs:complexType name="TermType" mixed="true">
        <xs:attribute name="MaximumQuantity" type="xs:string" use="optional"/>
        <xs:attribute name="EndDate" type="xs:string" use="optional"/>
        <xs:attribute name="TermsListID" type="StepID" use="optional"/>
        <xs:attribute name="MinimumQuantity" type="xs:string" use="optional"/>
        <xs:attribute name="LeadTime" type="xs:string" use="optional"/>
        <xs:attribute name="UnitID" type="StepUnitID" use="optional"/>
        <xs:attribute name="StartDate" type="xs:string" use="optional"/>
        <xs:attribute name="LotSize" type="xs:string" use="optional"/>
    </xs:complexType>


    <xs:complexType name="TermsType">
        <xs:sequence>
            <xs:element name="Term" type="TermType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="TermsListLinkType">
        <xs:attribute name="TermsListID" type="StepID" use="required"/>
    </xs:complexType>


    <xs:complexType name="TextType" mixed="true">
         <xs:sequence minOccurs="0">
             <xs:element name="Image"/>
         </xs:sequence>
     </xs:complexType>

    <xs:complexType name="TextElementType" mixed="true">
        <xs:attribute name="ExtendToFrameID" type="StepID" use="optional"/>
        <xs:attribute name="Text" type="xs:string" use="optional"/>
        <xs:attribute name="FrameID" type="StepID" use="optional"/>
    </xs:complexType>


    <xs:complexType name="TransformationLookupTableConfigurationType">
        <xs:annotation>
            <xs:documentation>Element hold content of a transformation lookup table configuration.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="ClassificationReference" type="ClassificationReferenceType" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object. Not required during import, in which
                    case new object is created using auto IDs.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="TransformationLookupTableConfigurationsType">
        <xs:annotation>
            <xs:documentation>Element that holds all transformation lookup table configurations.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TransformationLookupTableConfiguration" type="TransformationLookupTableConfigurationType"
                        minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="UnitType">
        <xs:annotation>
            <xs:documentation>
                A unit may contain conversions to other units.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="UnitConversion" type="UnitConversionType" minOccurs="0"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepUnitID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent unit group.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to references pointing to it or not.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>

        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="CollectionType">
        <xs:annotation>
            <xs:documentation>
                A Collection is a container of objects, either as a result of a query, or as a pick list.
                Objects themselves, of either kind, are not exported/imported. The queryURL is the only
                attribute of a Collection coming in and out of a STEP system this way.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepID" use="required"/>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The optional parentID is used for importing Collections into an already existing
                    hierarchy of CollectionGroups.
                    You cannot use this field for importing into an eCatalog.
                    If that is what you want then you need surrounding ECatalog tag.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="QueryURL" type="xs:string"/>
    </xs:complexType>


    <xs:complexType name="UnitConversionType">
        <xs:annotation>
            <xs:documentation>
                The UnitConversion element specifies a conversion to a base unit: BaseValue = Value * Factor + Offset
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Factor" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The factor in converting value to base unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BaseUnitID" type="StepUnitID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the base unit for this conversion.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Offset" type="xs:string" use="optional" default="0">
            <xs:annotation>
                <xs:documentation>
                    The offset in converting value to base unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="UnitFamilyType">
        <xs:annotation>
            <xs:documentation>
                A UnitFamily contains a number of units and other unit families
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="Unit" type="UnitType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepUnitID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false"/>
    </xs:complexType>

    <xs:complexType name="CollectionGroupType">
        <xs:annotation>
            <xs:documentation>
                The CollectionGroup is a container for Collections.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="CollectionGroup" type="CollectionGroupType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Collection" type="CollectionType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepID" use="required"/>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The optional parentID is used for importing CollectionGroups into an already existing
                    hierarchy of CollectionGroups.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="UnitLinkType">
        <xs:annotation>
            <xs:documentation>
                Links to a unit.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="UnitID" type="StepUnitID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the unit to link to.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="UnitListType">
        <xs:annotation>
            <xs:documentation>
                The list of units in the system
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="UnitFamily" type="UnitFamilyType"/>
                <xs:element name="Unit" type="UnitType"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CollectionListType">
        <xs:annotation>
            <xs:documentation>
                The CollectionList holds a list of CollectionGroups and/or Collections.
            </xs:documentation>
        </xs:annotation>
            <xs:sequence>
                <xs:element name="CollectionGroup" type="CollectionGroupType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="Collection" type="CollectionType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BusinessLibrariesType">
        <xs:annotation>
            <xs:documentation>
                A list of Business Library configurations.
            </xs:documentation>
        </xs:annotation>
            <xs:sequence>
                <xs:element name="BusinessRule" type="BusinessRuleType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BusinessRulesType">
        <xs:annotation>
            <xs:documentation>
                A list of Business Rule configurations.
            </xs:documentation>
        </xs:annotation>
            <xs:sequence>
                <xs:element name="BusinessRule" type="BusinessRuleType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BusinessRuleType">
        <xs:annotation>
            <xs:documentation>
                A configuration of a Business Rule.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SetupGroupLink" type="SetupGroupLinkType" minOccurs="0"/>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Description" type="BusinessRuleDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Dependency" type="BusinessRuleDependencyType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="FunctionDependency" type="BusinessFunctionDependencyType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="OnApprove" type="OnApproveType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Ignored for business rules of type Library.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ValidObjectTypes" type="ValidObjectTypesType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>

        <xs:attribute name="ID" type="StepBusinessRuleID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the Business Rule.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Scope" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The Scope of the Business Rule - required for business rules of type Action and Condition. Ignored for business rules of type Library.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Local"/>
                    <xs:enumeration value="Global"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="RunPrivileged" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Run privileged - without privilege check. Ignored for business rules of type Library.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Type" use="required">
            <xs:annotation>
                <xs:documentation>
                    The Type of the Business Rule.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Condition"/>
                    <xs:enumeration value="Action"/>
                    <xs:enumeration value="Library"/>
                    <xs:enumeration value="Function"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
    </xs:complexType>

    <xs:complexType name="ValidObjectTypeType">
        <xs:annotation>
            <xs:documentation>
                A valid object type for a business rule.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="ID" type="StepObjectTypeID">
            <xs:annotation>
                <xs:documentation>
                    The ID of the valid object type.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="ValidObjectTypesType">
        <xs:annotation>
            <xs:documentation>
                The sequence object types valid for a business rule.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="ValidObjectType" type="ValidObjectTypeType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>

        <xs:attribute name="AllObjectTypesValid" type="xs:boolean" use="required">
            <xs:annotation>
                <xs:documentation>
                    Indicator wheter all object types a valid for a business rule or not.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="LocalBusinessRuleType">
        <xs:annotation>
            <xs:documentation>
                A configuration of a Local Business Rule used within other objects; setup-groups and approval-info is superflous.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Description" type="BusinessRuleDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Dependency" type="BusinessRuleDependencyType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ValidObjectTypes" type="ValidObjectTypesType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>

        <xs:attribute name="ID" type="StepBusinessRuleID" use="required">
            <xs:annotation>
                <xs:documentation>
