            <xs:element name="ClassificationReference" type="ClassificationReferenceType" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object. Not required during import, in which
                    case new object is created using auto IDs.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="BulkUpdateConfigurationsType">
        <xs:annotation>
            <xs:documentation>Element that holds all bulk update configurations.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="BulkUpdateConfiguration" type="BulkUpdateConfigurationType" minOccurs="0"
                        maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="CellType">
        <xs:annotation>
            <xs:documentation>Element that holds table cell definition. Notice: Because of backward compatibility, we do unfortunately not follow the normal first character uppercase convention for attributeID.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ProductOrigin" type="ProductOriginType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="ClassificationOrigin" type="ClassificationOriginType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="EntityOrigin" type="EntityOriginType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AssetOrigin" type="AssetOriginType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AttributeOrigin" type="AttributeOriginType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Text" type="TextType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ProcessFlag" type="xs:string" use="optional"/>
        <xs:attribute name="VerticalSpan" type="xs:string" use="optional"/>
        <xs:attribute name="HorizontalSpan" type="xs:string" use="optional"/>
        <xs:attribute name="Column" type="xs:string" use="optional"/>
        <xs:attribute name="RuleAbove" type="xs:string" use="optional"/>
        <xs:attribute name="BackgroundColor" type="xs:string" use="optional"/>
        <xs:attribute name="RuleBelow" type="xs:string" use="optional"/>
        <xs:attribute name="RuleRight" type="xs:string" use="optional"/>
        <xs:attribute name="CellRotation" type="xs:string" use="optional"/>
        <xs:attribute name="RuleLeft" type="xs:string" use="optional"/>
        <xs:attribute name="TextStyle" type="xs:string" use="optional"/>
        <xs:attribute name="VerticalAlignment" type="xs:string" use="optional"/>
        <xs:attribute name="CellVerticalStoryDirection" type="xs:string" use="optional"/>
        <xs:attribute name="CellImageScale" type="xs:string" use="optional"/>
        <xs:attribute name="attributeID" type="StepAttributeID" use="optional"/>
    </xs:complexType>


    <xs:complexType name="ClassificationType">

        <xs:annotation>
            <xs:documentation>
                The Classification element lists its names, the linked attributes on this level, sub classification,
                cross-references to other classifications, metadata on classification and assets linked into this
                classification. This recursive definition of classifications spans the classification hierarchy. Note
                that products appear in their own hierarchy below the Products element.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="KeyValue" type="KeyValueType" minOccurs="0"/>
            <xs:element name="ParentKeyValue" type="ParentKeyValueType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Only exported when ParentID is also exported - ie. exported for root objects in export, and not exported for nested objects.
                        <Classification><SequenceProduct/></Classification>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="Name" type="NameType"/>
                <xs:element name="AttributeLink" type="AttributeLinkType"/>
                <xs:element name="DataContainerTypeLink" type="DataContainerTypeLinkType"/>
                <xs:element name="SequenceProduct" type="SequenceProductType">
                    <xs:annotation>
                        <xs:documentation>
                            Product sequences are used to show the sequence of child products under manually sorted classifications.
                            By default the sequences are not exported. And there is no option to turn it on using the export wiard.
                            In order for them to be exported, you must put it explicitly in the export recorder file:
                            <Classification><SequenceProduct/></Classification>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Classification" type="ClassificationType"/>
                <xs:element name="SuppressedClassificationCrossReference" type="SuppressedClassificationCrossReferenceType"/>
                <xs:element name="ProductReference" type="ProductReferenceType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>
                            If a Classification has Product to Classification links owned by the product,
                            information about these links will be exported under the classification using the ProductReference-tag.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ClassificationCrossReference" type="ClassificationCrossReferenceType"/>
                <xs:element name="SuppressedProductCrossReference" type="SuppressedProductCrossReferenceType"/>
                <xs:element name="ProductCrossReference" type="ProductCrossReferenceType"/>
                <xs:element name="SuppressedEntityCrossReference" type="SuppressedEntityCrossReferenceType"/>
                <xs:element name="EntityCrossReference" type="EntityCrossReferenceType"/>
                <xs:element name="SuppressedAssetCrossReference" type="SuppressedAssetCrossReferenceType"/>
                <xs:element name="AssetCrossReference" type="AssetCrossReferenceType"/>
                <xs:element name="MetaData" type="MetaDataType"/>
                <xs:element name="Tables" type="TablesType"/>
                <xs:element name="Asset" type="AssetType"/>
                <xs:element name="PublicationGroup" type="PublicationGroupType"/>
                <xs:element name="DeleteProductReference" type="DeleteProductReferenceType" minOccurs="0"
                            maxOccurs="unbounded"/>
                <xs:element name="DeleteSuppressedProductReference" type="DeleteSuppressedProductReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteClassificationCrossReference" type="DeleteClassificationCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteProductCrossReference" type="DeleteProductCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteEntityCrossReference" type="DeleteEntityCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteSuppressedClassificationCrossReference" type="DeleteSuppressedClassificationCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteSuppressedProductCrossReference" type="DeleteSuppressedProductCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteSuppressedEntityCrossReference" type="DeleteSuppressedEntityCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteSuppressedAssetCrossReference" type="DeleteSuppressedAssetCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteAttributeLink" type="DeleteAttributeLinkType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteDataContainerTypeLink" type="DeleteDataContainerTypeLinkType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="DeleteAssetCrossReference" type="DeleteAssetCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="CurrentTasks" type="CurrentTasksType"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="Type" use="optional" default="1">
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="SourcePos" type="xs:string" use="optional"/>
        <xs:attribute name="AnalyzerResult" type="xs:string" use="optional"/>
        <xs:attribute name="ParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The parent classification of this classification. Only required if not nested inside another
                    classification element
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="ID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object. Not required during import, in which
                    case new object is created.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="QualifierID" type="StepQualifierID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Identifies which contexts this object is visible in. If not specified the
                    current context is used
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Republished" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="UserTypeID" type="StepObjectTypeID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Identifies the object type of the object.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>

        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to references pointing to it or not.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="ClassificationBasketLinkType">
        <xs:choice minOccurs="0">
            <xs:element name="KeyValue" type="KeyValueType"/>
        </xs:choice>
        <xs:attribute name="ClassificationID" type="StepID" use="required"/>
