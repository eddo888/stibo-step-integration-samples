<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.stibosystems.com/step"
           xmlns="http://www.stibosystems.com/step"
           elementFormDefault="qualified" attributeFormDefault="unqualified">

    <xs:element name="STEP-ProductInformation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="AnalyzerResult" type="AnalyzerResultType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="ReplacementRules" type="ReplacementRulesType" minOccurs="0"/>
                <xs:element name="TagGroupList" type="TagGroupListType" minOccurs="0"/>
                <xs:element name="TagList" type="TagListType" minOccurs="0"/>
                <xs:element name="DimensionList" type="DimensionListType" minOccurs="0"/>
                <xs:element name="Qualifiers" type="QualifiersType" minOccurs="0"/>
                <xs:element name="ContextList" type="ContextListType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="CrossContexts" type="CrossContextsType" minOccurs="0"/>
                <xs:element name="GlobalSettings" type="GlobalSettingsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="UserTypes" type="UserTypesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="Keys" type="KeysType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="EdgeTypes" type="EdgeTypesType" minOccurs="0"/>
                <xs:element name="DerivedEventTypes" type="DerivedEventTypesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="CrossReferenceTypes" type="CrossReferenceTypesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="DeleteAssets" type="DeleteAssetsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="DeleteProducts" type="DeleteProductsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="DeleteEntities" type="DeleteEntitiesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="DeleteClassifications" type="DeleteClassificationsType" minOccurs="0"/>
                <xs:element name="UnitList" type="UnitListType" minOccurs="0"/>
                <xs:element name="CollectionList" type="CollectionListType" minOccurs="0"/>
                <xs:element name="ListOfValuesGroupList" type="ListOfValuesGroupListType" minOccurs="0"/>
                <xs:element name="ListsOfValues" type="ListsOfValuesType" minOccurs="0"/>
                <xs:element name="AttributeGroupList" type="AttributeGroupListType" minOccurs="0"/>
                <xs:element name="AttributeList" type="AttributeListType" minOccurs="0"/>
                <xs:element name="AttributeTransformationGroups" type="AttributeTransformationGroupsType"
                            minOccurs="0"/>
                <xs:element name="TableColors" type="TableColorsType" minOccurs="0"/>
                <xs:element name="TableRules" type="TableRulesType" minOccurs="0"/>
                <xs:element name="TableTypeGroupList" type="RowColumnTableGroupListType" minOccurs="0"/>
                <xs:element name="ColumnTypeGroupList" type="RowColumnTableGroupListType" minOccurs="0"/>
                <xs:element name="RowTypeGroupList" type="RowColumnTableGroupListType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="TableTypes" type="TableTypesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="ColumnTypes" type="ColumnTypesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="RowTypes" type="RowTypesType" minOccurs="0"/>
                <xs:element name="DataContainerTypes" type="DataContainerTypesType" minOccurs="0"/>
                <xs:element name="SetupGroups" type="SetupGroupsType" minOccurs="0"/>
                <xs:element name="SetupEntities" type="SetupEntitiesType" minOccurs="0"/>
                <xs:element name="BusinessLibraries" type="BusinessLibrariesType" minOccurs="0"/>
                <xs:element name="BusinessRules" type="BusinessRulesType" minOccurs="0"/>
                <xs:element name="STEPWorkflows" type="STEPWorkflowsType" minOccurs="0"/>
                <xs:element name="StatusFlags" type="StatusFlagsType" minOccurs="0"/>
                <xs:element name="MatchCodes" type="MatchCodesType" minOccurs="0"/>
                <xs:element name="MatchingAlgorithms" type="MatchingAlgorithmsType" minOccurs="0"/>
                <xs:element name="Classifications" type="ClassificationsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="PublicationGroups" type="PublicationGroupsType" minOccurs="0"/>
                <xs:element name="Assets" type="AssetsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="Entities" type="EntitiesType" minOccurs="0"/>
                <xs:element name="Products" type="ProductsType" minOccurs="0"/>
                <xs:element name="ActionSetList" type="ActionSetListType" minOccurs="0"/>
                <xs:element name="UserGroupList" type="UserGroupListType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="UserList" type="UserListType" minOccurs="0"/>
                <xs:element name="SystemSetup" type="SystemSetupType" minOccurs="0"/>
                <xs:element name="ImportConfigurations" type="ImportConfigurationsType" minOccurs="0"/>
                <xs:element name="ExportConfigurations" type="ExportConfigurationsType" minOccurs="0"/>
                <xs:element name="BulkUpdateConfigurations" type="BulkUpdateConfigurationsType" minOccurs="0"/>
                <xs:element name="TransformationLookupTableConfigurations"
                            type="TransformationLookupTableConfigurationsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="WebSites" type="WebSitesType" minOccurs="0"/>
                <xs:element name="ECatalogs" type="ECatalogsType" minOccurs="0"/>
                <xs:element name="Stateflows" type="STEPWorkflowsType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            @deprecated Replaced by STEPWorkflows.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PortalConfigurations" type="PortalConfigurationsType" minOccurs="0"/>
                <xs:element name="DeleteUnits" type="DeleteUnitsType" minOccurs="0"/>
                <xs:element name="DeleteAttributes" type="DeleteAttributesType" minOccurs="0"/>
                <xs:element name="DeleteListOfValues" type="DeleteListOfValuesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="EventQueues" type="EventQueuesType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="MissingObjects" type="MissingObjectsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="IntegrationEndpoints" type="IntegrationEndpointsType" minOccurs="0"/>
<!-- DOC -->    <xs:element name="EventProcessors" type="EventProcessorsType" minOccurs="0"/>
                <xs:element name="ChangePackages" type="ChangePackagesType" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="ContextID" type="StepQualifierID" use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Specifies the ID of the context that provides the content selection for an export.
                        During import, it identifies the context to which objects without an explicit QualifierID
                        specification will be associated
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AutoApprove" type="YesNoType" use="optional" default="N"/>
            <xs:attribute name="AutoInitiate" type="YesNoType" use="optional" default="N"/>
            <xs:attribute name="StateflowImportEvent" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        @deprecated Replaced by STEPWorkflowImportEvent.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="STEPWorkflowImportEvent" type="YesNoType" use="optional" default="N"/>
            <xs:attribute name="ExportTransactionID" type="StepID" use="optional" default="-1"/>
            <xs:attribute name="ReplaceProductValues" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        @deprecated Replaced by ReplaceValues in ReplacementRules structure.

                        Either “Y” or “N”. Indicates if all values of product attributes for each product should be
                        replaced by the values specified in the file. Values are per default replace in all contexts,
                        however setting the environment variable PII_PRESERVE_VALUE_ON_REPLACE to “true” will only clear
                        values in given context. A more detailed replacement specification can be made by using the
                        ReplacementRules sub element – please contact Stibo if that is required.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AllowInvalidValues" type="YesNoType" use="optional" default="N"/>
            <xs:attribute name="ReplaceAttributeLinks" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        Either “Y” or “N”. Indicates if all attribute links for each classification should be replaced
                        by the links provided in the file. A more detailed replacement specification can be made by
                        using the ReplacementRules sub element – please contact Stibo if that is required.
                    </xs:documentation>
                </xs:annotation>

            </xs:attribute>
            <xs:attribute name="ReplaceProductToProductCrossReferences" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        @deprecated Replaced by ReplaceCrossReferences in ReplacementRulesStructure.

                        Either “Y” or “N”. Indicates if all product references for each product should be replaced by
                        references provided in the file. A more detailed replacement specification can be made by using
                        the ReplacementRules sub element – please contact Stibo if that is required.
                    </xs:documentation>
                </xs:annotation>

            </xs:attribute>
            <xs:attribute name="ExportContext" type="StepQualifierID" use="optional"/>
            <xs:attribute name="ReplaceTerms" use="optional" default="CLEAR_ONLY_FOR_IMPORTED_PRODUCTS">
                <xs:simpleType>
                    <xs:restriction base="xs:NMTOKEN">
                        <xs:enumeration value="CLEAR_ENTIRE_TERMSLISTS"/>
                        <xs:enumeration value="CLEAR_ONLY_FOR_IMPORTED_PRODUCTS"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ReplaceClassificationToClassificationCrossReferences" type="YesNoType"
                          use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>Either “Y” or “N”. Indicates if all classification cross references for
                        each classification should be replaced by the references provided in the file. A more detailed
                        replacement specification can be made by using the ReplacementRules sub element – please
                        contact Stibo if that is required
                    </xs:documentation>
                </xs:annotation>

            </xs:attribute>
            <xs:attribute name="ReplaceOverrideToSubProductReferences" use="optional" default="N" type="YesNoType">
                <xs:annotation>
                    <xs:documentation>
                        Either “Y” or “N”. Indicates whether or not imported OverrideSubProduct should replace
                        existing sub products of productoverrides.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="SingleUpdateMode" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        Either “Y” or “N”. Indicates whether or not the whole import will run in single update mode.
                        Specifying “Y” is required for certain import operations, e.g. adding dimension dependencies
                        and changing validation. When specifying “N”, operations requiring single update mode to execute
                        will be skipped and warnings will be issued.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="WorkspaceID" type="StepID" use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Specifies the ID of the workspace that provides the content selection for an export. During
                        import, it identifies the workspace to which objects will be associated.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ReplaceProductToClassificationReferences" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        Either “Y” or “N”. Indicates if all classification references for each product should be
                        replaced by references provided in the file. A more detailed replacement specification can be
                        made by using the ReplacementRules sub element – please contact Stibo if that is required.
                    </xs:documentation>
                </xs:annotation>

            </xs:attribute>
            <xs:attribute name="ReplaceClassificationValues" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>
                        @deprecated Replaced by ReplaceValues in ReplacementRules structure.

                        Either “Y” or “N”. Indicates if all classification meta data for each
                        classification should be replaced by the meta data provided in the
                        file. A more detailed replacement specification can be made by using the ReplacementRules sub
                        element – please contact Stibo if that is required.
                    </xs:documentation>
                </xs:annotation>

            </xs:attribute>

            <xs:attribute name="ReplaceValidUserTypesForAttributes" type="YesNoType" use="optional" default="N">
                <xs:annotation>
                    <xs:documentation>Either “Y” or “N”. Indicates if existing valid object types should be deleted
                        if they are not present in the import, and thus replacing the entire validity span of an
                        Attribute.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>

            <xs:attribute name="ExportTime" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO: restriction på dateformatet -->
                        Specifies the starting time for an export in the format “YYYY-MM-DD HH:MM:SS”, eg “1999-10-15
                        12:00:00”.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>

            <xs:attribute name="UseContextLocale" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Used for export only. Determines if output should be converted according to Locale-setting(s) as defined on the context(s) begin exported.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>

        </xs:complexType>
    </xs:element>

    <xs:simpleType name="StepID">
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StepAttributeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepUnitID"><xs:restriction base="StepID"/></xs:simpleType>                         <!-- also used for UnitFamily -->
    <xs:simpleType name="StepObjectTypeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepDataContainerTypeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepLOVID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepQualifierID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepLinkTypeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepReferenceTypeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepInboundEndPointID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepOutboundEndPointID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepGatewayEndPointID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepSetupGroupID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepBusinessRuleID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepWorkflowID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepEventQueueID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepAssetPushConfigurationID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepKeyID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepMatchingAlgorithmID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepMatchCodeID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepEventProcessorID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepSetupEntityID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepPortalConfigurationID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepUserID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepUserGroupID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepActionSetID"><xs:restriction base="StepID"/></xs:simpleType>
    <xs:simpleType name="StepActionID"><xs:restriction base="StepID"/></xs:simpleType>

    <xs:simpleType name="YesNoType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TrueFalseType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true"/>
            <xs:enumeration value="false"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ReferenceTargetLockPolicyType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Strict"/>
            <xs:enumeration value="Relaxed"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EntityRevisabilityType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Global"/>
            <xs:enumeration value="Workspace"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ActionLinkType">
        <xs:annotation>
            <xs:documentation>
                A link to an action.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="ActionID" type="StepActionID" use="required">
            <xs:annotation>
                   <xs:documentation>
                       The ID of the action.
                  </xs:documentation>
               </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="ParentChildRelationType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="TARGETASCHILD"/>
            <xs:enumeration value="SOURCEASCHILD"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ActionSetType">
        <xs:annotation>
            <xs:documentation>
                A set of actions that can be granted to a user through privilegerules.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="ActionLink" type="ActionLinkType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepActionSetID" use="required"/>
        <xs:attribute name="Type" use="required">
            <xs:annotation>
                <xs:documentation>
                    Determines whether the Action Set belongs to the System Setup tab, or is a general PIM user action.
                    Has one of the values 'setup' or 'user' accordingly.
                </xs:documentation>
            </xs:annotation>

            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="setup"/>
                    <xs:enumeration value="user"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
    </xs:complexType>


    <xs:complexType name="ActionSetListType">
        <xs:annotation>
            <xs:documentation>
                A list of action sets.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ActionSet" type="ActionSetType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AnalyzerElementType">
        <xs:annotation>
            <xs:documentation>Represents an item in a result list. ItemID identifies the item and refers to the element
                AnalyzerItem. If the item refers to a step node, one of the following attributes will be present:
                AssetID, ProductID, or ClassificationID
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="ProductID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                     If item corresponds to a product, the values will be the ID of the
                     corresponding product.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AssetID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    If item corresponds to an asset, the values will be the ID of the corresponding asset.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ItemID" type="StepID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Identifies the item in result list. Refers to AnalyzerItem that may contain
                    additional information about the result from the AnalyzerPLugin. Typically
                    the URL is used as identifier.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ClassificationID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    If item corresponds to a classification, the values will be the ID of the
                    corresponding classification.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AnalyzerItemType">
        <xs:annotation>
            <xs:documentation>
                Represents an item analyzed by the AnalyzerPlugin. E.g. a changed or affected classification or product.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AnalyzerProp" type="AnalyzerPropType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Identifies the analyzer item – typically the URL of the node in question is used as ID.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="StepType" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    <!-- TODO: mangler restriction på value -->
                    Identifies the type of Step object. Possible values: Classification, Asset, Product.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="StepID" type="StepID" use="optional">
           <xs:annotation>
                <xs:documentation>
                    Identifies the ID of the corresponding Step object.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AnalyzerItemsType">
        <xs:annotation>
            <xs:documentation>
                Contains all AnalyzerItems.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AnalyzerItem" type="AnalyzerItemType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AnalyzerListType">
        <xs:annotation>
            <xs:documentation>
                Represents a list of items in one of the analyzer result lists. The ‘name’ identifies the list, e.g.
                “item.modified”, “items.referencing.added” etc
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AnalyzerElement" type="AnalyzerElementType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Identifies the result list.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AnalyzerListsType">
        <xs:annotation>
            <xs:documentation>
                Represents lists of AnalyzerList.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AnalyzerList" type="AnalyzerListType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AnalyzerPropType" mixed="true">
        <xs:annotation>
            <xs:documentation>
                Represents additional information calculated by the AnalyzerPlugin.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="PropID" type="StepID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Identifies the property. E.g. ‘difference.result’, ‘mode.content’,
                    ‘sequence’
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AnalyzerResultType">
        <xs:annotation>
            <xs:documentation>
                Contains AnalyzerLists and AnalyzerItems.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AnalyzerLists" type="AnalyzerListsType"/>
            <xs:element name="AnalyzerItems" type="AnalyzerItemsType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AssetType">
        <xs:annotation>
            <xs:documentation>
                Describes an asset in PIM. The Asset element lists its names, classification references and values. Note
                that assets don’t have their own hierarchy. Therefore they must be nested inside a classification
                element and/or have a ClassificationReference sub element. DTP Document assets may contain pages that
                have been created by a DTP client.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="KeyValue" type="KeyValueType" minOccurs="0"/>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AssetPushLocation" type="AssetPushLocationType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="SuppressedClassificationReference" type="SuppressedClassificationReferenceType"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="ClassificationReference" type="ClassificationReferenceType" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="SuppressedEntityCrossReference" type="SuppressedEntityCrossReferenceType"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="EntityCrossReference" type="EntityCrossReferenceType" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="Values" type="ValuesType" minOccurs="0"/>
            <xs:element name="CurrentTasks" type="CurrentTasksType" minOccurs="0"/>
            <xs:element name="DeleteClassificationReference" type="DeleteClassificationReferenceType" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="DeleteEntityCrossReference" type="DeleteEntityCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="DeleteSuppressedClassificationReference" type="DeleteSuppressedClassificationReferenceType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="DeleteSuppressedEntityCrossReference" type="DeleteSuppressedEntityCrossReferenceType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Pages" type="PagesType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AssetContent" type="AssetContentType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AssetBinaryContent" type="AssetBinaryContentType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="SourcePos" type="xs:string" use="optional"/>
        <xs:attribute name="ID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Global identification of the object. Not required during import, in which case new object is created
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Republished" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="UserTypeID" type="StepObjectTypeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    Identifies the object type of the object.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AnalyzerResult" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    <!-- TODO: mangler restriction på value -->
                    WebPublisher specific attribute. Identifies whether the asset in question is
                    present on the website. Possible values: included, excluded.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to references pointing to it or not.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

    </xs:complexType>

    <xs:complexType name="AssetContentType">
        <xs:annotation>
            <xs:documentation>
                Holds all the conversion information for a single asset for a single context.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="AssetContentSpecification" type="AssetContentSpecificationType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>

        <xs:attribute name="QualifierID" type="xs:string" use="optional"/>

    </xs:complexType>

    <xs:complexType name="AssetContentSpecificationType">
        <xs:annotation>
            <xs:documentation>
                Holds all the asset conversion information for a single asset in a single context.
            </xs:documentation>
        </xs:annotation>

        <xs:attribute name="ImageConversionConfigurationID" type="xs:string" use="optional"/>
        <xs:attribute name="IncludesBinaryContents" type="xs:boolean" use="optional"/>
        <xs:attribute name="RelativeURL" type="xs:string" use="optional"/>

    </xs:complexType>

    <xs:complexType name="AssetBinaryContentType">
        <xs:annotation>
            <xs:documentation>
                Holds the base64 encoded binary asset data for a single asset conversion for a single context
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Checksum" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="BinaryContent" type="ConfigurationType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>

        <xs:attribute name="QualifierID" type="xs:string" use="optional"/>
        <xs:attribute name="ImageConversionConfigurationID" type="xs:string" use="optional"/>
        <xs:attribute name="MIMEType" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:simpleType name="SelectedType">
        <xs:annotation>
            <xs:documentation>
                When exporting from STEP, this flag indicates if this object was explicitly or implicitly part of the selected export set (ie. selected or children).
                Notice the default value true, which must be respected when Selected is not present in the file
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean"/>
    </xs:simpleType>

    <xs:complexType name="AssetBasketLinkType">
        <xs:choice minOccurs="0">
            <xs:element name="KeyValue" type="KeyValueType"/>
        </xs:choice>
        <xs:attribute name="AssetID" type="StepID" use="required"/>
    </xs:complexType>


    <xs:complexType name="AssetCrossReferenceType">
        <xs:annotation>
            <xs:documentation>
                A product to asset cross-reference.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="KeyValue" type="KeyValueType" minOccurs="0"/>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
                <xs:element name="Asset" type="AssetType" minOccurs="0"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="Type" type="StepLinkTypeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The type of the reference. The list of valid reference types can be
                    accessed through the CrossReferenceTypes (one of the AssetCrossReferenceType elements) located below
                    the main element in the XML file
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="AssetID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The ID of the asset to refer to.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AnalyzerResult" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    WebPublisher specific attribute. Identifies whether the referenced asset is
                    present on the website. Possible values: included, excluded
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="QualifierID" type="StepQualifierID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Identifies which contexts this object is visible in. If not specified the current
                    context is used.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AssetCrossReferenceTypeType">
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="AttributeLink" type="AttributeLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="DimensionLink" type="DimensionLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AttributeGroupLink" type="AttributeGroupLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="UserTypeLink" type="UserTypeLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="TargetUserTypeLink" type="TargetUserTypeLinkType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="Inherited" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="ID" type="StepLinkTypeID" use="required"/>
        <xs:attribute name="Revised" use="optional" default="true" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Completeness" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    DEPRECATED: not used anymore after the introduction of Completeness Metrics in STEP 7.4 (see documentation on Completeness Metrics and migration from old pre-7.4 completeness functionality).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MultiValued" use="optional" default="true" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Mandatory" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Accumulated" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>
                    When exporting from STEP, this flag indicates the object was referenced in other exported data.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
    </xs:complexType>


    <xs:complexType name="AssetElementType">
        <xs:choice minOccurs="0">
            <xs:element name="RuleBasedLayout" type="RuleBasedLayoutType"/>
            <xs:element name="KeyValue" type="KeyValueType"/>
            <xs:element name="TemplateLayout" type="TemplateLayoutType"/>
            <xs:element name="PickupLayout" type="PickupLayoutType"/>
        </xs:choice>
        <xs:attribute name="ExtendToFrameID" type="StepID" use="optional"/>
        <xs:attribute name="FrameID" type="StepID" use="optional"/>
        <xs:attribute name="AssetID" type="StepID" use="required"/>
    </xs:complexType>


    <xs:complexType name="AssetMarkerElementType">
        <xs:attribute name="ExtendToFrameID" type="StepID" use="optional"/>
        <xs:attribute name="FrameID" type="StepID" use="optional"/>
    </xs:complexType>


    <xs:complexType name="AssetPushLocationType" mixed="true">
        <xs:attribute name="Status" type="xs:string"/>
        <xs:attribute name="ConfigurationID" type="StepID" use="required"/>
    </xs:complexType>


    <xs:complexType name="AssetsType">
        <xs:annotation>
            <xs:documentation>
                Element that holds all top level asset
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Asset" type="AssetType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>

        <xs:attribute name="RejectNewAssets" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>Whether or not to reject new asset during import.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="DefaultParentID" type="StepID" use="optional">
            <xs:annotation>
                <xs:documentation>Identifies a default parent classification for new assets during import
                    that do not have an explicit classification reference.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="DefaultUserTypeID" type="StepObjectTypeID" use="optional">
            <xs:annotation>
                <xs:documentation>Identifies the default object type of the asset in no object type is given for a new asset during import</xs:documentation>
            </xs:annotation>
        </xs:attribute>

    </xs:complexType>


    <xs:complexType name="AttributeType">
        <xs:annotation>
            <xs:documentation>
                This element represents an attribute in the system. The validation or ListOfValueLink restricts the
                valid values of this attribute. The dimension links is the dimension dependencies for values of this
                attribute. UserTypeLink and LinkType list the objects that are allowed to have values of this attribute.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:choice>
                <xs:element name="Validation" type="ValidationType"/>
                <xs:element name="ListOfValueLink" type="ListOfValueLinkType"/>
            </xs:choice>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="ValueTemplate" type="ValueTemplateType"/>
                <xs:element name="UnitTemplate" type="UnitTemplateType"/>
            </xs:choice>
            <xs:element name="DimensionLink" type="DimensionLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="AttributeGroupLink" type="AttributeGroupLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="UserTypeLink" type="UserTypeLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="LinkType" type="LinkTypeType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="ValueFilter" type="ValueFilterType" minOccurs="0"/>
            <xs:element name="CurrentTasks" type="CurrentTasksType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="HierarchicalFiltering" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Whether or not it is allowed to put filters on product-attribute links.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ClassificationHierarchicalFiltering" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Whether or not it is allowed to put filters on classification-attribute links.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ExternallyMaintained" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Whether or not values of this attribute are externally maintained
                    or not. If not externally maintained full revision control of values of
                    this attribute are available. On import this will only have effect
                    when creating new attributes (cannot update this information for
                    existing attributes on import).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="FullTextIndexed" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Whether or not values of this attribute should be full text indexed
                    or not. Will always be if max length greater than 100. Must be
                    either true or false. On import this will only have effect when creating new attributes (cannot
                    update this information for existing
                    attributes on import).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MultiValued" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Whether attribute is multi valued or not. Must have one of the
                    following values: true or false. On import this will only have effect
                    when creating new attributes (cannot update this information for
                    existing attributes on import).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ID" type="StepAttributeID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The ID of the attribute object. Not required during import, in which case an auto ID generation must be set up for object type.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DefaultUnitID" type="StepUnitID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The ID of the default unit for values of this attribute. On import
                    this will only have effect when creating new attributes (cannot
                    update this information for existing attributes on import).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ProductMode" use="optional" default="Normal">
            <xs:annotation>
                <xs:documentation>
                    Whether this attribute is “ClassValidated”, “Property” or
                    “Normal”. On import this will only have effect when creating new
                    attributes (cannot update this information for existing attributes
                    on import).
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Normal"/>
                    <xs:enumeration value="Property"/>
                    <xs:enumeration value="ClassValidated"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Mandatory" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether or not this attribute is mandatory.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Derived" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether or not this attribute is derived. If derived it makes sense to use the template
                    attribute.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>

        <xs:attribute name="Referenced" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether or not an attribute has been referenced.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="CryptographicKeyID" use="optional" type="StepID">
            <xs:annotation>
                <xs:documentation>
                    The ID of the cryptographic key if the attribute is encrypted. It cannot be changed if the
                    attribute has values.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Completeness" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    DEPRECATED: not used anymore after the introduction of Completeness Metrics in STEP 7.4 (see documentation on Completeness Metrics and migration from old pre-7.4 completeness functionality).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType"/>
    </xs:complexType>


    <xs:complexType name="AttributeSequenceType">
        <xs:annotation>
            <xs:documentation>
                The sequence of attributes in a manually sorted attribute group. On import then any attributes not mentioned in the sequence will be left in the beginning of the sequence.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AttributeLink" type="AttributeLinkType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AttributeGroupType">
        <xs:annotation>
            <xs:documentation>
                An attribute group holding a number of attributes or attribute groups.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0"/>
            <xs:element name="AttributeSequence" type="AttributeSequenceType" minOccurs="0"/>
            <xs:element name="AttributeGroup" type="AttributeGroupType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepAttributeID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the attribute group object. Not required during import, in which case an auto ID generation must be set up for object type.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentID" type="StepAttributeID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the parent attribute group.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ShowInWorkbench" type="TrueFalseType" use="optional" default="true">
            <xs:annotation>
                <xs:documentation>
                    True if this attribute group should be shown in the workbench when editing
                    references and values. Defaults to true if not set (for example on upgraded databases).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ExcludeFromProfiling" type="TrueFalseType" use="optional">
            <xs:annotation>
                <xs:documentation>
                    DEPRECATED: not used anymore after the introduction of attribute group filtering in profile configurations.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to
                    references pointing to it or not.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ManuallySorted" type="TrueFalseType" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>
                    True if the attributes and reference types are manually sorted in this group.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Selected" type="SelectedType" use="optional" default="true"/>
    </xs:complexType>


    <xs:complexType name="AttributeGroupLinkType">
        <xs:annotation>
            <xs:documentation>
                Links an object into a attribute group.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="AttributeGroupID" type="StepAttributeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the attribute group to link to.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>

    </xs:complexType>

    <xs:complexType name="AttributeGroupListType">
        <xs:annotation>
            <xs:documentation>
                The list of all attribute group.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AttributeGroup" type="AttributeGroupType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AttributeLinkType">
        <xs:annotation>
            <xs:documentation>
                The AttributeLink element links an attribute into a classification or a product; it is also used to validate
                an attribute against a link type or cross reference type.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="MetaData" type="MetaDataType" minOccurs="0"/>
            <xs:element name="ValueFilter" type="ValueFilterType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Inherited" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Only relevant during export when nested inside a product or classification.
                    If present during import then the attribute link is simply ignored.
                    If this attribute is present then link is inherited - will not be exported for local links
                    (thus only numbers 1 and upward will be exported - 1 indicating inherited from immediate parent).
                    The integer value of this attribute indicates far up this link is inherited from.
                    In cross context exports it might be relevant to have this number to find the most relevant link for a given context.
                    NOTE: will only inherit inside hierarchy of same equal node types - ie. will not inherit from classification to product hierarchy.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="QualifierID" type="StepQualifierID" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Identifies which contexts this object is visible in. If not specified the current
                    context is used
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Mandatory" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether or not this attribute is mandatory for products or assets
                    below this object. Must have on of the following values: true or false.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="AttributeID" type="StepAttributeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the attribute to link into this object.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to
                    references pointing to it or not.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="DataContainerTypeLinkType">
        <xs:annotation>
            <xs:documentation>
                The DataContainerTypeLink element links a data container type into a classification or a product.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Changed" use="optional" default="false" type="TrueFalseType">
        </xs:attribute>
        <xs:attribute name="Inherited" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Only relevant during export when nested inside a product or classification.
                    If present during import then the link is simply ignored.
                    If this container type is present then link is inherited - will not be exported for local links
                    (thus only numbers 1 and upward will be exported - 1 indicating inherited from immediate parent).
                    The integer value of this container type indicates far up this link is inherited from.
                    In cross context exports it might be relevant to have this number to find the most relevant link for a given context.
                    NOTE: will only inherit inside hierarchy of same equal node types - ie. will not inherit from classification to product hierarchy.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <!--<xs:attribute name="Mandatory" use="optional" default="false" type="TrueFalseType">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether or not this container type is mandatory for products
                    below this object. Must have on of the following values: true or false.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        -->
        <xs:attribute name="DataContainerTypeID" type="StepDataContainerTypeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the data container type to link into this object.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Referenced" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>When exporting from STEP, this flag indicates if this object was extracted due to
                    references pointing to it or not.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AttributeValidationType">
        <xs:annotation>
            <xs:documentation>
                The AttributeValidationType is used for describing a validity relation to an attribute.
            </xs:documentation>
        </xs:annotation>

        <xs:attribute name="AttributeID" type="StepAttributeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the attribute.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AttributeListType">
        <xs:annotation>
            <xs:documentation>
                The list of attributes.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="Attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AttributeTransformationType">
        <xs:annotation>
            <xs:documentation>
                An attribute transformation is an object where a transformation of an attribute value can be
                specified. The transformation can then be linked to attributes. Using a transformation can be done in
                tables or the output system (currently only indesign).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="UserTypeLink" type="UserTypeLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AttributeGroupLink" type="AttributeGroupLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="AttributeLink" type="AttributeLinkType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Configuration" type="ConfigurationType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="StepAttributeID" use="required">
            <xs:annotation>
                <xs:documentation>
                    The ID of the attribute transformation.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="AttributeTransformationGroupType">
        <xs:annotation>
            <xs:documentation>
